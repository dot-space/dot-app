<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui.home.HomeFragment">

    <com.google.android.material.imageview.ShapeableImageView
        android:id="@+id/map_imageview"
        android:layout_width="375dp"
        android:layout_height="225dp"
        android:layout_marginTop="40dp"
        android:src="@drawable/nmims_shirpur_map_rounded"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0"
        app:layout_goneMarginEnd="5dp" />

    <ImageView
        android:id="@+id/github_connect"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_marginStart="28dp"
        android:src="@drawable/github"
        app:layout_constraintBottom_toTopOf="@+id/facebook_connect"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/ws_address"
        app:layout_constraintVertical_bias="0.709" />

    <ImageView
        android:id="@+id/discord_connect"
        android:layout_width="60dp"
        android:layout_height="60dp"

        android:src="@drawable/discord_1"
        app:layout_constraintBottom_toTopOf="@+id/www_connect"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.468"
        app:layout_constraintStart_toEndOf="@+id/gmail_connect"
        app:layout_constraintTop_toBottomOf="@+id/ws_address"
        app:layout_constraintVertical_bias="0.709" />

    <ImageView
        android:id="@+id/instagram_connect"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_marginBottom="72dp"
        android:src="@drawable/instagram"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.117"
        app:layout_constraintStart_toEndOf="@+id/facebook_connect" />

    <ImageView
        android:id="@+id/www_connect"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_marginBottom="72dp"
        android:src="@drawable/personal_website"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.797"
        app:layout_constraintStart_toEndOf="@+id/instagram_connect" />

    <ImageView
        android:id="@+id/linkedin_connect"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_marginEnd="160dp"

        android:src="@drawable/linkedin"
        app:layout_constraintBottom_toTopOf="@+id/instagram_connect"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.333"
        app:layout_constraintStart_toEndOf="@+id/github_connect"
        app:layout_constraintTop_toBottomOf="@+id/ws_address"
        app:layout_constraintVertical_bias="0.709" />

    <ImageView
        android:id="@+id/facebook_connect"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_marginBottom="72dp"
        android:src="@drawable/facebook"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.091"
        app:layout_constraintStart_toStartOf="parent" />

    <ImageView
        android:id="@+id/gmail_connect"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:src="@drawable/gmail_1"

        app:layout_constraintBottom_toTopOf="@+id/twitter_connect"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.627"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/ws_address"
        app:layout_constraintVertical_bias="0.709" />

    <ImageView
        android:id="@+id/twitter_connect"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_marginBottom="72dp"
        android:src="@drawable/twitter"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/www_connect"
        app:layout_constraintHorizontal_bias="0.493"
        app:layout_constraintStart_toEndOf="@+id/instagram_connect" />

    <TextView
        android:id="@+id/ws_address"
        style="@style/Widget.DotSpace.TextView"
        android:layout_width="348dp"
        android:layout_height="187dp"
        android:layout_marginTop="90dp"
        android:fontFamily="@font/actor"
        android:text="@string/ws_address"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.253"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/map_imageview" />

    <TextView
        android:id="@+id/our_den"
        style="@style/Widget.DotSpace.TextView"
        android:layout_width="115dp"
        android:layout_height="30dp"
        android:layout_marginTop="16dp"
        android:layout_marginBottom="5dp"
        android:fontFamily="@font/actor"
        android:textSize="25sp"
        android:textStyle="bold"
        android:text="@string/our_den"
        app:layout_constraintBottom_toTopOf="@+id/ws_address"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.054"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/map_imageview"
        app:layout_constraintVertical_bias="1.0" />

</androidx.constraintlayout.widget.ConstraintLayout>